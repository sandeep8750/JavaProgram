import java.util.Scanner;

public class Palindrome {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int n = new Scanner(System.in).nextInt();
		int rem, rev = 0 ; 
		while(n>0) {
			
			rem = n%10; 
			rev = rev *10 + rem; 
			n = n/10 ;
		}
		if(rev == n ) {
			System.out.print("palindrome");
		}
	}

}
